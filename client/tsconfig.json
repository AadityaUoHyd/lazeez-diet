{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
    "compilerOptions": {
      "baseUrl": ".",
      "paths": {
      "@/*": ["./src/*"]
    },
      "module": "ESNext",
      "target": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "types": ["vite/client"]
    }
  
  
}
